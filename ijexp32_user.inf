[ShellExtReg1]
HKCU,"Software\Classes\CLSID\%ShellExtCLSID1%",,,"%ShellExtDesc1%"
HKCU,"Software\Classes\CLSID\%ShellExtCLSID1%\InProcServer32",,,"%16410%\%SubDir%\ijexp32.dll"
HKCU,"Software\Classes\CLSID\%ShellExtCLSID1%\InProcServer32","ThreadingModel",,"Apartment"
HKCU,"Software\Classes\dllfile\%PropSheetHdlr%\%ShellExtCLSID1%",,,"%ShellExtDesc1%"
HKCU,"Software\Classes\exefile\%PropSheetHdlr%\%ShellExtCLSID1%",,,"%ShellExtDesc1%"
HKCU,"Software\Classes\sysfile\%PropSheetHdlr%\%ShellExtCLSID1%",,,"%ShellExtDesc1%"
HKCU,"%ShellExtNT%","%ShellExtCLSID1%",,"%ShellExtDesc1%"

[ShellExtReg2]
HKCU,"Software\Classes\CLSID\%ShellExtCLSID2%",,,"%ShellExtDesc2%"
HKCU,"Software\Classes\CLSID\%ShellExtCLSID2%\InProcServer32",,,"%16410%\%SubDir%\ijexp32.dll"
HKCU,"Software\Classes\CLSID\%ShellExtCLSID2%\InProcServer32","ThreadingModel",,"Apartment"
HKCU,"Software\Classes\dllfile\%PropSheetHdlr%\%ShellExtCLSID2%",,,"%ShellExtDesc2%"
HKCU,"%ShellExtNT%","%ShellExtCLSID2%",,"%ShellExtDesc2%"

[ShellExtReg3]
HKCU,"Software\Classes\CLSID\%ShellExtCLSID3%",,,"%ShellExtDesc3%"
HKCU,"Software\Classes\CLSID\%ShellExtCLSID3%\InProcServer32",,,"%16410%\%SubDir%\ijexp32.dll"
HKCU,"Software\Classes\CLSID\%ShellExtCLSID3%\InProcServer32","ThreadingModel",,"Apartment"
HKCU,"Software\Classes\dllfile\%PropSheetHdlr%\%ShellExtCLSID3%",,,"%ShellExtDesc3%"
HKCU,"Software\Classes\exefile\%PropSheetHdlr%\%ShellExtCLSID3%",,,"%ShellExtDesc3%"
HKCU,"Software\Classes\sysfile\%PropSheetHdlr%\%ShellExtCLSID3%",,,"%ShellExtDesc3%"
HKCU,"%ShellExtNT%","%ShellExtCLSID3%",,"%ShellExtDesc3%"

[UninstReg1]
HKCU,"%Uninstall%",,,
HKCU,"%Uninstall%","DisplayName",,"%UninstDispName%"
HKCU,"%Uninstall%","UninstallString",,"rundll.exe setupx.dll,InstallHinfSection DefaultUnInstall 4 %16410%\%SubDir%\ijexp32_user.inf"

[UninstReg1NT]
HKCU,"%Uninstall%",,,
HKCU,"%Uninstall%","DisplayName",,"%UninstDispName%"
HKCU,"%Uninstall%","UninstallString",,"RunDll32 syssetup.dll,SetupInfObjectInstallAction DefaultUnInstall.NT 4 %16410%\%SubDir%\ijexp32_user.inf"

[DefaultInstall]
CopyFiles = Files1
AddReg    = ShellExtReg1, ShellExtReg2, ShellExtReg3, UninstReg1

[DefaultInstall.NT]
CopyFiles = Files1
AddReg    = ShellExtReg1, ShellExtReg2, ShellExtReg3, UninstReg1NT

[DefaultUninstall]
DelFiles = Files1
DelReg   = ShellExtReg1, ShellExtReg2, ShellExtReg3, UninstReg1

[DefaultUninstall.NT]
DelFiles = Files1
DelReg   = ShellExtReg1, ShellExtReg2, ShellExtReg3, UninstReg1NT

