[Version]
Signature  = "$Windows NT$"
SetupClass = BASE

[Strings]
SubDir         = "ijexp32"
ShellExtCLSID1 = "{00000001-23D0-0001-8000-004026419740}"
ShellExtDesc1  = "i.j Shell Property Sheet ExeHdr 32"
ShellExtCLSID2 = "{00000002-23D0-0001-8000-004026419740}"
ShellExtDesc2  = "i.j Shell Property Sheet Export 32"
ShellExtCLSID3 = "{00000003-23D0-0001-8000-004026419740}"
ShellExtDesc3  = "i.j Shell Property Sheet Import 32"
PropSheetHdlr  = "shellex\PropertySheetHandlers"
ShellExtNT     = "SoftWare\Microsoft\Windows NT\CurrentVersion\ShellExtensions\Approved"
Uninstall      = "SoftWare\Microsoft\Windows\CurrentVersion\Uninstall\ijexp32"
UninstDispName = "i.j Shell Property Sheets Export/Import 32 (Remove only)"

[SourceDisksNames]
55 = "ijexp32 v1.01",,

[SourceDisksFiles]
ijexp32.dll = 55
ijexp32_user.inf = 55
comctl32.exp = 55
;ijdll32.exp = 55
;ijgdi32.exp = 55
mfc30.exp = 55
mfc40.exp = 55
